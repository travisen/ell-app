<% include partials/header %>
	<div id="content-padding" class="container">
		<h2>Add a fun place you went to for points!</h2>
		<form action="/add-visit" method="POST">

		  <div class="form-group">
		    <label>Your Name</label>
		    <input type="text" name="name" class="form-control" placeholder="First Name" required>
		  </div>
		  <div class="form-group">
		  	<label>Place Name</label>
		  	<input id="places" name="place" class="form-control" type="text" list="places" required>
		  </div>
		  <div class="form-group">
		    <label>Date</label>
		    <input type="date" name="date" class="form-control" placeholder="Place" min="2017-06-01" max="<%=currentDate %>"  value="<%=currentDate %>" required>
		  </div>
		  <button type="submit" class="btn btn-primary">Submit</button>
		</form>
	</div>

	<!--<script> $(".chosen-select").chosen() </script>-->

<% include partials/footer %>